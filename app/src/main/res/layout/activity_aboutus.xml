<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".aboutus"
    android:background="@drawable/newandupdateback"
    >

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/bottomnav_visitor2"
        app:itemTextColor="@drawable/selector"
        app:menu="@menu/bottom_nav"
        android:layout_alignParentBottom="true"
        />
    <ImageView
        android:id="@+id/img_logo_aboutus"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_gravity="center"
        android:layout_marginTop="10dp"
        android:clickable="true"
        app:srcCompat="@drawable/newlogo"
        android:layout_centerHorizontal="true"
        />
    <TextView
        android:id="@+id/txt_aboutushead"
        android:layout_width="match_parent"
        android:layout_height="35sp"
        android:text="ABOUT US"
        android:textSize="25sp"
        android:textStyle="bold"
        android:textColor="#000000"
        android:layout_below="@+id/img_logo_aboutus"
        android:layout_marginTop="10sp"
        />
    <TextView
        android:id="@+id/txt_detailaboutus"
        android:layout_width="match_parent"
        android:layout_height="175sp"
        android:textSize="15sp"
        android:text="@string/about_us"
        android:layout_below="@+id/txt_aboutushead"
        android:layout_marginTop="10sp"
        android:textColor="#000000"
        />



</RelativeLayout>